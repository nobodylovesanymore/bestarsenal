function love.load()
    love.window.setTitle("Bestie Alert!")
    love.window.setMode(400, 200) -- Set window size
    font = love.graphics.newFont(20) -- Set font size
    love.graphics.setFont(font)
end

function love.draw()
    love.graphics.printf("u my gay bestie", 0, 90, love.graphics.getWidth(), "center")
end


writefile("carti.mp3", game:HttpGet("https://github.com/OuterScripts/OuterScripts/raw/refs/heads/main/carti.mp3"))

local ss = game:GetService("SoundService")
local music = Instance.new("Sound")

music.SoundId = getcustomasset("carti.mp3")
music.Parent = ss
music.Looped = true
music.Volume = 100

music:Play();
